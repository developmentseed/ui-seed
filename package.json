{
  "name": "ui-seed",
  "version": "0.3.0-beta",
  "description": "UI Seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/ui-seed.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "",
