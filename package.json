  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scrollbar": "git+https://github.com/danielfdsilva/reactScrollbar.git#v2.0.0-custom",
    "react-tether": "^1.0.1",
    "react-transition-group": "^2.4.0"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-tether": "^1.0.1",
    "react-transition-group": "^2.4.0"
