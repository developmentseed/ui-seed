    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.24.4",
    "browserify": "^16.2.2",
    "collecticons-processor": "^3.1.0",
    "del": "^3.0.0",
    "envify": "^4.1.0",
    "eslint": "^5.4.0",
