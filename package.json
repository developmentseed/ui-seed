    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "fancy-log": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.1",
    "gulp-ejs": "^3.0.1",
    "gulp-exit": "0.0.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.2.0",
